<div class="container">
    <div class="contact">
        <div class="contact_form">
            <div class="contact_tittle">Contact US</div>
            <div class="contact_form_input">
                <div class="row">
                    <div class="col-12">
                        <div class="input-group input-group-lg">
              <span class="input-group-text" id="inputGroup-sizing-lg"
              >Full Name</span
              >
                            <input
                                    type="text"
                                    class="form-control"
                                    aria-label="Sizing example input"
                                    aria-describedby="inputGroup-sizing-lg"
                            />
                        </div>
                    </div>
                    <div class="col-6" style="margin-top: 30px">
                        <div class="input-group input-group-lg">
              <span class="input-group-text" id="inputGroup-sizing-lg"
              >Email</span
              >
                            <input
                                    type="text"
                                    class="form-control"
                                    aria-label="Sizing example input"
                                    aria-describedby="inputGroup-sizing-lg"
                            />
                        </div>
                    </div>
                    <div class="col-6" style="margin-top: 30px">
                        <div class="input-group input-group-lg">
              <span class="input-group-text" id="inputGroup-sizing-lg"
              >Phone Number</span
              >
                            <input
                                    type="text"
                                    class="form-control"
                                    aria-label="Sizing example input"
                                    aria-describedby="inputGroup-sizing-lg"
                            />
                        </div>
                    </div>
                    <div class="col-12" style="margin-top: 30px">
                        <h4 style="font-weight: normal">Message:</h4>
                        <div class="input-group input-group-lg">
              <textarea
                      style="border-radius: 10px; margin-bottom: 50px"
                      class="message_contact"
                      name=""
                      id=""
                      cols="300"
                      rows="10"
              ></textarea>
                        </div>
                    </div>
                    <br />
                    <button
                            type="button"
                            class="btn btn-success"
                            style="margin-bottom: 50px"
                    >
                        Success
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>
